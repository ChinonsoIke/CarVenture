@model IEnumerable<UserResponseDto>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>S/N</th>
                <th>Name</th>
                <th>Email</th>
                <th>Registered</th>
            </tr>
        </thead>
        <tbody>
            @{
                int count = 0;

                if(Model.Count() > 0)
                {
                    foreach(var user in Model)
                    {
                        <tr>
                            <td>@{count++;} @count</td>
                            <td>@user.FullName</td>
                            <td>@user.Email</td>
                            <td>@user.CreatedAt</td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>