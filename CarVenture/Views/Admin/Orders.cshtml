@model IEnumerable<OrderResponseDto>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>S/N</th>
                <th>Car ID</th>
                <th>User ID</th>
                <th>Created</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @{
                int count = 0;

                if(Model.Count() > 0)
                {
                    foreach(var order in Model)
                    {
                        <tr>
                            <td>@{count++;} @count</td>
                            <td>@order.CarId</td>
                            <td>@order.UserId</td>
                            <td>@order.CreatedAt</td>
                            <td>@order.Status</td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>